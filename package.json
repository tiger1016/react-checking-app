{
  "name": "petcheck-web",
  "version": "1.0.0",
  "description": "Petcheck Web",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "del -rf dist && cross-env NODE_ENV=production yarn webpack",
    "deploy:staging": "yarn build && yarn upload:staging",
    "e2e": "node ./e2e/exec.js",
    "e2e:mobile": "node ./e2e/exec.js --mobile",
    "e2e:doctor": "mocha e2e/doctor.js",
    "e2e:v1:customers": "mocha e2e/v1/run.js --customers",
    "e2e:v1:invoicing": "mocha e2e/v1/run.js --invoicing",
    "e2e:v1:payrolls": "mocha e2e/v1/run.js --payrolls",
    "e2e:v1:reports": "mocha e2e/v1/run.js --reports",
    "e2e:v1:scheduler": "mocha e2e/v1/run.js --scheduler",
    "e2e:v1:settings": "mocha e2e/v1/run.js --settings --addons && mocha e2e/v1/run.js --settings --customer-billing && mocha e2e/v1/run.js --settings --services && mocha e2e/v1/run.js --settings --scheduler",
    "e2e:v1:staff": "mocha e2e/v1/run.js --staff",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "pre-push": "scripts/pre-push.bash",
    "ssh:staging": "scripts/ssh-to-server.bash",
    "start": "cross-env NODE_ENV=development WEBPACK_BUILD_TARGET=local PORT=3301 webpack-dev-server",
    "upload:staging": "scripts/upload-to-staging.bash",
    "webpack": "webpack --progress"
  },
  "author": "Petcheck Technology",
  "license": "ISC",
  "standard": {
    "globals": [
      "describe",
      "it",
      "Image",
      "expect"
    ],
    "ignore": [
      "custom_plugins/"
    ],
    "parser": "babel-eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn pre-push"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-transform": "^3.0.0",
    "chalk": "^2.4.2",
    "chromedriver": "^75.1.0",
    "circular-dependency-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^4.0.0",
    "geckodriver": "^1.16.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "mocha": "^6.1.4",
    "properties-reader": "^0.3.1",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "yargs": "^13.3.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.5.0",
    "axios": "^0.19.0",
    "braintree-web": "^3.46.0",
    "copy-to-clipboard": "^3.2.0",
    "fuse.js": "^3.4.5",
    "google-map-react": "^1.1.4",
    "moment": "^2.29.2",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.26",
    "print-js": "^1.0.61",
    "rc-progress": "^2.5.1",
    "rc-slider": "^8.6.13",
    "react": "^16.8.6",
    "react-autosize-textarea": "^7.0.0",
    "react-big-calendar": "^0.22.0",
    "react-datepicker": "^1.6.0",
    "react-dnd": "^9.0.1",
    "react-dnd-html5-backend": "^9.0.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.5",
    "react-number-format": "^4.0.8",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.1.0",
    "react-responsive": "^7.0.0",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.2.1",
    "react-slick": "^0.24.0",
    "react-spinkit": "^3.0.0",
    "react-table": "^6.10.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^5.3.1",
    "react-tooltip": "^3.10.0",
    "react-visibility-sensor": "^5.1.0",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-compress": "^4.2.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rm": "^0.1.8",
    "sweetalert-react": "^0.4.11",
    "text-mask-addons": "^3.8.0"
  }
}
